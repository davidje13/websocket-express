{
  "name": "websocket-express",
  "version": "3.1.2",
  "description": "express and ws combined",
  "files": [
    "build/**/*",
    "index.d.ts"
  ],
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "format": "prettier --write .",
    "test": "rm -rf build || true; lean-test --parallel-suites && npm run build && packagetest/run.sh && prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidje13/websocket-express.git"
  },
  "keywords": [
    "express",
    "ws",
    "WebSocket",
    "WebSockets"
  ],
  "author": "David Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidje13/websocket-express/issues"
  },
  "homepage": "https://github.com/davidje13/websocket-express#readme",
  "peerDependencies": {
    "express": "4.x || ~5.0.0-beta.1 || 5.x",
    "ws": "7.x || 8.x"
  },
  "devDependencies": {
    "@types/express": "4.x",
    "@types/ws": "8.x",
    "express": "4.x",
    "lean-test": "2.x",
    "prettier": "3.3.3",
    "rollup": "4.x",
    "supertest": "7.x",
    "superwstest": "2.x",
    "ws": "8.x"
  }
}
